{
    "logger": {
        "console": {
            "enabled": true
        },
        "file": {
            "enabled": false,
            "path": "logs/logs.txt",
            "interval": "day"
        },
        "seq": {
            "enabled": true,
            "url": "http://ubuntu-docker-host:5341",
            "apiKey": "secret"
        },
        "metrics": {
            "enabled": true,
            "influxEnabled": false,
            "prometheusEnabled": true,
            "influxUrl": "http://ubuntu-docker-host:8086",
            "database": "local"
        }
    }
}